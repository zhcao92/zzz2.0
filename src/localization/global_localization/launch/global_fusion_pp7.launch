<launch>

  <!-- global_fusion_pp7 调整车体坐标系到前左上(FLU),  无rviz -->
  <param name="debug" type="bool" value="false" />

  <!-- 北偏西取正 -->
  <param name="yaw_n2w" type="bool" value="false" />

  <!---input topics -->
  <param name="gnss_inspvax_topic" value="/gnss/inspvax" />
  <param name="gnss_rawimu_topic" value="/gnss/rawimu" />
  <param name="gnss_bestposb_topic" value="/gnss/bestposb" />
  <param name="gnss_bestvelb_topic" value="/gnss/bestvelb" />
  <param name="gnss_heading2b_topic" value="/gnss/heading2b" />

  <!---output topics -->
  <param name="gps_fix_topic" value="/localization/gps/fix" />
  <param name="gps_vel_topic" value="/localization/gps/vel" />
  <param name="gps_odom_topic" value="/localization/gps/odom" />
  <param name="gps_imu_topic" value="/localization/imu/data" />
  <param name="gps_pos_type_topic" value=" /localization/gps/pos_type" />
  <param name="gps_nav_status_topic" value=" /localization/gps/nav_status" />
  <param name="gps_time_ref_topic" value=" /localization/gps/time_ref" />

  <!-- 初始位置UTM0 -->
  <param name="utm0_x" type="double" value="442866.361698" />
  <param name="utm0_y" type="double" value="4427904.89691" />

  <!--- run-node -->
  <node pkg="global_localization" type="global_fusion_pp7_node" name="global_fusion_pp7_node" output="screen">
  </node>

  <!--- Sim Time -->
  <param name="/use_sim_time" value="false" />
  <node pkg="tf" type="static_transform_publisher" name="world_to_map" args="0 0 0  0   0  0 /utm    /map 100" />


</launch>
